{
  "name": "xmldom",
  "version": "0.4.0-dev",
  "description": "A pure JavaScript W3C standard-based (XML DOM Level 2 Core) DOMParser and XMLSerializer module.",
  "keywords": [
    "w3c",
    "dom",
    "xml",
    "parser",
    "javascript",
    "DOMParser",
    "XMLSerializer"
  ],
  "author": "jindw <jindw@xidea.org> (http://www.xidea.org)",
  "homepage": "https://github.com/xmldom/xmldom",
  "repository": {
    "type": "git",
    "url": "git://github.com/xmldom/xmldom.git"
  },
  "main": "lib/dom-parser.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "pretest": "XMLDOM_ASSERT_STRICT=1 npm run test:assert",
    "test": "vows 'test/**/*.vows.js'",
    "test:assert": "vows test/assert.vows.js",
    "start": "nodemon --watch package.json --watch lib --watch test --exec 'npm --silent run test'",
    "stryker": "stryker run"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@stryker-mutator/core": "^3.3.0",
    "@stryker-mutator/javascript-mutator": "^3.3.0",
