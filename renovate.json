{
  "extends": [
    "config:base",
    ":preserveSemverRanges",
    ":ignoreUnstable",
    ":respectLatest",
    ":semanticCommitsDisabled"
  ],
  "commitMessageExtra": "-> {{#if isMajor}}{{{newMajor}}}{{else}}{{#if isSingleVersion}}{{{toVersion}}}{{else}}{{{newValue}}}{{/if}}{{/if}} - {{depType}}",
  "commitMessageTopic": "{{depName}}",
  "rangeStrategy": "bump"
}
